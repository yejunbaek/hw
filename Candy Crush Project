import random
import time
points = 0
trial = 5

print("Welcome to the New Branded Candy Crush (NBCC)") #welcoming
time.sleep(2)#wait 2 seconds
#options
print("<0> Leave")
print("<1> How to play")
print("<2> Play game")
print("<3> Credits")
controls = input("What would you like to do?: ") #ask what the user wants to do
while controls != "0": #while loop to make the user control what they want to do
    if controls == "1":
        #show how to play the game
        print("HOW TO PLAY NEW BRANDED CANDY CRUSH")
        time.sleep(1)
        print("1. You choose 2 numbers. They would be the boundaries of what numbers would be shown on the board. NOTE: They can only be inbetween 1 and 10 and the first one must be smaller than the second one.")
        time.sleep(5)
        print("2. A board would be shown and you must choose the row or column using the given following: A or a, B or b, C or c, D or d, E or e, 1, 2, 3, and 4!")
        time.sleep(5)
        print("3. Once you chose your row or column, if there are a 3 consecutive numbers, then they would be CRUSHED and replaced with new ones")
        time.sleep(5)
        print("4. There is a point system and ranking system at the end to see how good you really are.")
        time.sleep(3)
        print("5. There is a 'shuffle' or 'Shuffle' command that uses 10 points and shuffles the board")
        time.sleep(3)
        print("That is all for now! Thank you for playing and hope you have a great time!")
        time.sleep(3)
        print("Welcome to the New Branded Candy Crush (NBCC)")
        print("<1> How to play")
        print("<2> Play game")
        print("<3> Credits")
        controls = input("What would you like to do?: ") #returning to while loop
