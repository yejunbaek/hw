                        print(1," ",randomcombo1,randomcombo2,randomcombo3,randomcombo4,randomcombo5)
                        print(2," ",randomcombo6,randomcombo7,randomcombo8,randomcombo9,randomcombo10)
                        print(3," ",randomcombo11,randomcombo12,randomcombo13,randomcombo14,randomcombo15)
                        print(4," ",randomcombo16,randomcombo17,randomcombo18,randomcombo19,randomcombo20)
                        userinput = input("Enter what you would like to break (0 to leave): ")
                        

                    elif trial == 0:
                        print("This is your last turn")
                        
                        print("    A B C D E")
                        print(1," ",randomcombo1,randomcombo2,randomcombo3,randomcombo4,randomcombo5)
                        print(2," ",randomcombo6,randomcombo7,randomcombo8,randomcombo9,randomcombo10)
                        print(3," ",randomcombo11,randomcombo12,randomcombo13,randomcombo14,randomcombo15)
                        print(4," ",randomcombo16,randomcombo17,randomcombo18,randomcombo19,randomcombo20)
                    elif trial < 0: 
                        print("CONGRATS!!!")
                        time.sleep(2)
                        print("You have ended with",points,"points!")
                        time.sleep(2)
                        if points <= 30:
                            print("You are Iron")
                        elif 30 < points <= 40:
                            print("You are Silver")
                        elif 40 < points <= 100:
                            print("You are Gold")
                        elif 100 < points:
                            print("You are a Legend")
                        print("Goodbye!")
                        userinput = "0"
                        points = 0
                        trial = 5
            elif userinput == "E" or userinput == "e":
                if column[16] == column[18] and column[17] == column[18]:
                    print("You got a combo! You get 3 points!")
                    points += 3
                    print("You have",points,"points")
                    randomcombo5 = int(random.randint(number,number2))
                    randomcombo10 = int(random.randint(number,number2))
                    randomcombo15 = int(random.randint(number,number2))
                    column = randomcombo1,randomcombo6,randomcombo11,randomcombo16,randomcombo2,randomcombo7,randomcombo12,randomcombo17,randomcombo3,randomcombo8,randomcombo13,randomcombo18,randomcombo4,randomcombo9,randomcombo14,randomcombo19,randomcombo5,randomcombo10,randomcombo15,randomcombo20      

                    row = randomcombo1,randomcombo2,randomcombo3,randomcombo4,randomcombo5,randomcombo6,randomcombo7,randomcombo8,randomcombo9,randomcombo10,randomcombo11,randomcombo12,randomcombo13,randomcombo14,randomcombo15,randomcombo16,randomcombo17,randomcombo18,randomcombo19,randomcombo20

                    print("    A B C D E")
                    print(1," ",randomcombo1,randomcombo2,randomcombo3,randomcombo4,randomcombo5)
                    print(2," ",randomcombo6,randomcombo7,randomcombo8,randomcombo9,randomcombo10)
                    print(3," ",randomcombo11,randomcombo12,randomcombo13,randomcombo14,randomcombo15)
                    print(4," ",randomcombo16,randomcombo17,randomcombo18,randomcombo19,randomcombo20)
                    userinput = input("Enter what you would like to break (0 to leave): ")
                elif column[17] == column[19] and column[18] == column[19]:
                    print("You got a combo! You get 3 points!")
                    points += 3
                    print("You have",points,"points")
                    randomcombo10 = int(random.randint(number,number2))
                    randomcombo15 = int(random.randint(number,number2))
                    randomcombo20 = int(random.randint(number,number2))
                    column = randomcombo1,randomcombo6,randomcombo11,randomcombo16,randomcombo2,randomcombo7,randomcombo12,randomcombo17,randomcombo3,randomcombo8,randomcombo13,randomcombo18,randomcombo4,randomcombo9,randomcombo14,randomcombo19,randomcombo5,randomcombo10,randomcombo15,randomcombo20      

                    row = randomcombo1,randomcombo2,randomcombo3,randomcombo4,randomcombo5,randomcombo6,randomcombo7,randomcombo8,randomcombo9,randomcombo10,randomcombo11,randomcombo12,randomcombo13,randomcombo14,randomcombo15,randomcombo16,randomcombo17,randomcombo18,randomcombo19,randomcombo20

                    print("    A B C D E")
                    print(1," ",randomcombo1,randomcombo2,randomcombo3,randomcombo4,randomcombo5)
                    print(2," ",randomcombo6,randomcombo7,randomcombo8,randomcombo9,randomcombo10)
                    print(3," ",randomcombo11,randomcombo12,randomcombo13,randomcombo14,randomcombo15)
                    print(4," ",randomcombo16,randomcombo17,randomcombo18,randomcombo19,randomcombo20)
                    userinput = input("Enter what you would like to break (0 to leave): ")

                elif column[16] != column[18] and column[17] != column[18] or column[17] != column[19] and column[18] != column[19]: 
                    trial -= 1
                    if trial > 0:
                        print("Move not possible! Try again")
                        print("You have",trial,"tries left")
                        print("    A B C D E")
                        print(1," ",randomcombo1,randomcombo2,randomcombo3,randomcombo4,randomcombo5)
                        print(2," ",randomcombo6,randomcombo7,randomcombo8,randomcombo9,randomcombo10)
                        print(3," ",randomcombo11,randomcombo12,randomcombo13,randomcombo14,randomcombo15)
                        print(4," ",randomcombo16,randomcombo17,randomcombo18,randomcombo19,randomcombo20)
                        userinput = input("Enter what you would like to break (0 to leave): ")
                        

                    elif trial == 0:
                        print("This is your last turn")
                        print("    A B C D E")
                        print(1," ",randomcombo1,randomcombo2,randomcombo3,randomcombo4,randomcombo5)
                        print(2," ",randomcombo6,randomcombo7,randomcombo8,randomcombo9,randomcombo10)
                        print(3," ",randomcombo11,randomcombo12,randomcombo13,randomcombo14,randomcombo15)
                        print(4," ",randomcombo16,randomcombo17,randomcombo18,randomcombo19,randomcombo20)
                    elif trial < 0:

                        print("CONGRATS!!!")
                        time.sleep(2)
                        print("You have ended with",points,"points!")
                        time.sleep(2)
                        if points <= 30:
                            print("You are Iron")
                        elif 30 < points <= 40:
                            print("You are Silver")
                        elif 40 < points <= 100:
                            print("You are Gold")
                        elif 100 < points:
                            print("You are a Legend")
                        print("Goodbye!")
                        userinput = "0"
                        points = 0
                        trial = 5
            elif userinput == "shuffle" or "Shuffle": #if user inputs shuffle
                userinput2 = input("Do you want to spend 10 points to shuffle the board?: ") #asking if they want to use 10 points to shuffle
                if userinput2 == "yes" or userinput2 == "Yes": #get user response
                    if points < 10: #if less than 10 then continue
                        print("You don't have enough points!") #inform the user that they don't have enough
                        print("Returning back . . .")
                        time.sleep(2)
                        #print board
                        print("    A B C D E")
                        print(1," ",randomcombo1,randomcombo2,randomcombo3,randomcombo4,randomcombo5)
                        print(2," ",randomcombo6,randomcombo7,randomcombo8,randomcombo9,randomcombo10)
                        print(3," ",randomcombo11,randomcombo12,randomcombo13,randomcombo14,randomcombo15)
                        print(4," ",randomcombo16,randomcombo17,randomcombo18,randomcombo19,randomcombo20)
                        userinput = input("Enter what you would like to break (0 to leave): ") #return to while loop
                    elif points > 10: #if it's greater than 10
                        print("You spent 10 points to shuffle the board . . . ") #inform that they can shuffle
                        points -= 10 #remove 10 points
                        print("You have",points,"points left!") #show how many points are left
                        time.sleep(1)
                        print("Shuffling table")
                        #remaking all the variables
                        randomcombo1 = int(random.randint(number,number2))
                        randomcombo2 = int(random.randint(number,number2))
                        randomcombo3 = int(random.randint(number,number2))
                        randomcombo4 = int(random.randint(number,number2))
                        randomcombo5 = int(random.randint(number,number2))
                        randomcombo6 = int(random.randint(number,number2))
                        randomcombo7 = int(random.randint(number,number2))
                        randomcombo8 = int(random.randint(number,number2))
                        randomcombo9 = int(random.randint(number,number2))
                        randomcombo10 = int(random.randint(number,number2))
                        randomcombo11 = int(random.randint(number,number2))
                        randomcombo12 = int(random.randint(number,number2))
                        randomcombo13 = int(random.randint(number,number2))
                        randomcombo14 = int(random.randint(number,number2))
                        randomcombo15 = int(random.randint(number,number2))
                        randomcombo16 = int(random.randint(number,number2))
                        randomcombo17 = int(random.randint(number,number2))
                        randomcombo18 = int(random.randint(number,number2))
                        randomcombo19 = int(random.randint(number,number2))
                        randomcombo20 = int(random.randint(number,number2))
                        column = randomcombo1,randomcombo6,randomcombo11,randomcombo16,randomcombo2,randomcombo7,randomcombo12,randomcombo17,randomcombo3,randomcombo8,randomcombo13,randomcombo18,randomcombo4,randomcombo9,randomcombo14,randomcombo19,randomcombo5,randomcombo10,randomcombo15,randomcombo20      

                        row = randomcombo1,randomcombo2,randomcombo3,randomcombo4,randomcombo5,randomcombo6,randomcombo7,randomcombo8,randomcombo9,randomcombo10,randomcombo11,randomcombo12,randomcombo13,randomcombo14,randomcombo15,randomcombo16,randomcombo17,randomcombo18,randomcombo19,randomcombo20

                        time.sleep(2)
                        #printing board
                        print("    A B C D E")
                        print(1," ",randomcombo1,randomcombo2,randomcombo3,randomcombo4,randomcombo5)
                        print(2," ",randomcombo6,randomcombo7,randomcombo8,randomcombo9,randomcombo10)
                        print(3," ",randomcombo11,randomcombo12,randomcombo13,randomcombo14,randomcombo15)
                        print(4," ",randomcombo16,randomcombo17,randomcombo18,randomcombo19,randomcombo20)
                        userinput = input("Enter what you would like to break (0 to leave): ") #returning to while loop
                    else: #if they put some weird output
                        print("Invalid Input!") #inform that they put an invalid output
                        userinput2 = input("Do you want to spend 10 points to shuffle the board?: ") #return to if statement
                elif userinput2 == "no" or "No": # if they say no
                    print("Returning back to original board . . . ") #return to original board
                    time.sleep(2)
                    column = randomcombo1,randomcombo6,randomcombo11,randomcombo16,randomcombo2,randomcombo7,randomcombo12,randomcombo17,randomcombo3,randomcombo8,randomcombo13,randomcombo18,randomcombo4,randomcombo9,randomcombo14,randomcombo19,randomcombo5,randomcombo10,randomcombo15,randomcombo20      

                    row = randomcombo1,randomcombo2,randomcombo3,randomcombo4,randomcombo5,randomcombo6,randomcombo7,randomcombo8,randomcombo9,randomcombo10,randomcombo11,randomcombo12,randomcombo13,randomcombo14,randomcombo15,randomcombo16,randomcombo17,randomcombo18,randomcombo19,randomcombo20

                    time.sleep(2)
                    #printing board
                    print("    A B C D E")
                    print(1," ",randomcombo1,randomcombo2,randomcombo3,randomcombo4,randomcombo5)
                    print(2," ",randomcombo6,randomcombo7,randomcombo8,randomcombo9,randomcombo10)
                    print(3," ",randomcombo11,randomcombo12,randomcombo13,randomcombo14,randomcombo15)
                    print(4," ",randomcombo16,randomcombo17,randomcombo18,randomcombo19,randomcombo20)
                    userinput = input("Enter what you would like to break (0 to leave): ")

            else: #if they put an invalid output
                print("Invalid Answer") #inform them
                time.sleep(1)
                #printing board
                print("    A B C D E")
                print(1," ",randomcombo1,randomcombo2,randomcombo3,randomcombo4,randomcombo5)
                print(2," ",randomcombo6,randomcombo7,randomcombo8,randomcombo9,randomcombo10)
                print(3," ",randomcombo11,randomcombo12,randomcombo13,randomcombo14,randomcombo15)
                print(4," ",randomcombo16,randomcombo17,randomcombo18,randomcombo19,randomcombo20)
                userinput = input("Enter what you would like to break (0 to leave): ")
print("Goodbye!") #sending a farewell message
