        print(2," ",randomcombo6,randomcombo7,randomcombo8,randomcombo9,randomcombo10)
        print(3," ",randomcombo11,randomcombo12,randomcombo13,randomcombo14,randomcombo15)
        print(4," ",randomcombo16,randomcombo17,randomcombo18,randomcombo19,randomcombo20)



