        print("NAME: Joseph Baek")
        print("COURSE: ICS20")
        print("PROJECT: Candy Crush Programming Project")
        print("<1> How to play")
        print("<2> Play game")
        print("<3> Credits")
        controls = input("What would you like to do?: ") #returning to while loop
    elif controls == "2": #play the game
        print("Enter the numbers you would like to have on your board:")
        #finding the numbers
        number = int(input("Enter a number lower than 10 and bigger than 0: ")) 
        number2 = int(input("Enter a second number lower than 10 and bigger than first number: "))
        while not (0 < number < 10 and 0 < number2 < 10): #seeing if what they inputed is invalid
            print("Invalid Input! Retry")
            number = int(input("Enter a number lower than 10 and bigger than 0: "))
            number2 = int(input("Enter a second number lower than 10 and bigger than your first number: "))
        #if the 2 numbers are greater than 2 units apart then make sure if that's what they wanted
        if number2 > 2:
            question = input("Are you sure that you want to choose this number? It would be hard!: ")
            if question == "yes" or question == "Yes" or question == "1":
                print("Alright . . . . ") #continuing 
                time.sleep(1)
            elif question == "no" or question == "No" or question == "0": #returning to while loop
                number = int(input("Enter a number lower than 10 and bigger than 0: "))
                number2 = int(input("Enter a second number lower than 10 and bigger than your first number: "))
        #making new variables
        randomcombo1 = int(random.randint(number,number2))
        randomcombo2 = int(random.randint(number,number2))
        randomcombo3 = int(random.randint(number,number2))
        randomcombo4 = int(random.randint(number,number2))
        randomcombo5 = int(random.randint(number,number2))
        randomcombo6 = int(random.randint(number,number2))
        randomcombo7 = int(random.randint(number,number2))
        randomcombo8 = int(random.randint(number,number2))
        randomcombo9 = int(random.randint(number,number2))
        randomcombo10 = int(random.randint(number,number2))
        randomcombo11 = int(random.randint(number,number2))
        randomcombo12 = int(random.randint(number,number2))
        randomcombo13 = int(random.randint(number,number2))
        randomcombo14 = int(random.randint(number,number2))
        randomcombo15 = int(random.randint(number,number2))
        randomcombo16 = int(random.randint(number,number2))
        randomcombo17 = int(random.randint(number,number2))
        randomcombo18 = int(random.randint(number,number2))
        randomcombo19 = int(random.randint(number,number2))
        randomcombo20 = int(random.randint(number,number2))
